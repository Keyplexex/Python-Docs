{
    "name": "python-docs",
    "displayName": "Python Docs",
    "description": "Open Python documentation for selected function",
    "version": "0.0.",
    "publisher": "Keyplex",
    "activationEvents": [
     "onCommand:pythonDocs.openDocs"
    ],
    "main": "out/extension.js",
    "contributes": {
     "commands": [
      {
       "command": "pythonDocs.openDocs",
       "title": "Open Python Docs"
      }
     ]
    },
    "scripts": {
     "vscode:prepublish": "tsc -p ./",
     "compile": "tsc -p ./",
     "watch": "tsc -w -p ./",
     "package": "vsce package"
    },
    "devDependencies": {
     "@types/node": "^22.9.0",
     "@types/vscode": "^1.74.0",
     "typescript": "^4.8.4",
     "vscode": "^1.1.34" 
    },
    "engines": {
     "node": ">=14.18.0", 
     "vscode": "^1.74.0" 
    }  
   }